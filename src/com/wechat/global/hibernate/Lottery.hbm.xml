<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping
	package="com.wechat.global.hibernate">
	<class name="Lottery" table="LOTTERY">
		<id name="report_id"  column="report_id"   />
		<property name="red_ball_1" type="string" column="red_ball_1" />
		<property name="red_ball_2" type="string" column="red_ball_2" />
		<property name="red_ball_3" type="string" column="red_ball_3" />
		<property name="red_ball_4" type="string" column="red_ball_4" />
		<property name="red_ball_5" type="string" column="red_ball_5" />
		<property name="blue_ball_1" type="string" column="blue_ball_1" />
		<property name="blue_ball_2" type="string" column="blue_ball_2" />
	</class>

</hibernate-mapping>
